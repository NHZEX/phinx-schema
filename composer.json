{
    "name": "zxin/phinx-schema",
    "type": "library",
    "description": "phinx schema extend",
    "keywords": [],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "php-cs-fixer/shim": "^3",
        "phpunit/phpunit": "^8|^9",
        "rector/rector": "^2.0",
        "robmorgan/phinx": "~0.12.5|~0.13.1|~0.14",
        "symfony/var-dumper": "^5.0|^6.0|^7.0"
    },
    "suggest": {
        "robmorgan/phinx": "must require or phar"
    },
    "autoload": {
        "psr-4": {
            "Zxin\\Phinx\\Schema\\": "src"
        },
        "files": [
            "src/func.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zxin\\Tests\\": "tests"
        }
    },
    "extra": {},
    "config": {
        "sort-packages": true
    }
}
